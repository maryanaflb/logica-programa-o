programa
{
	
	funcao inicio()
	{
		inteiro nota
		escreva("informe a nota do aluno:")
		leia(nota)
		se((nota>=0)e(nota>3)){
			escreva("conceito E!")
		}senao se((nota>=3)e(nota<=5)){
			escreva("conceito D!")
		}senao se((nota==6)ou(nota==7)){
			escreva("conceito C!")
		}senao se(nota==10){
			escreva("conceito B!")
		}senao se(nota==10){
			escreva("conceito A!")
		}senao{
			escreva("ERROR")
		}
	}
}
